GET .kibana*/_search
{
  "query": {
    "bool": {
      "filter": [
        {
          "terms": {
            "alert.alertTypeId": [
              ".es-query",
              ".geo-containment"
            ]
          }
        }
      ],
      "must_not": {
        "exists": {
          "field": "references"
        }
      }
    }
  }
}
